<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>iot_project_grader</name>
  <version>1.0.0</version>
  <description>Iot Project Grader. Displays the score of the project in a convenient window using the DearPyGui libraries</description>
  <maintainer email="trombetti@di.uniroma1.it">fede3751</maintainer>
  <license>Apache 2.0</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <exec_depend>dearpygui</exec_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
